{
  "appId": "com.zouzhiqiang.todo",
  "asar": true,
  "productName": "我的日程管理",
  "directories": {
    "output": "release/${version}"
  },
  "files": [
    "public",
    "dist",
    "app"
  ],
  "mac": {
    "artifactName": "${productName}_${version}.${ext}",
    "target": [
      "dmg"
    ]
  },
  "win": {
    "target": [
      {
        "target": "nsis",
        "arch": [
          "x64"
        ]
      }
    ],
    "artifactName": "${productName}_${version}.${ext}"
  },
  "nsis": {
    "oneClick": false,
    "perMachine": false,
    "allowToChangeInstallationDirectory": true,
    "deleteAppDataOnUninstall": false
  }
}